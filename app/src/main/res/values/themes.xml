<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.Proprioception" parent="Theme.Material3.Light.NoActionBar">
        <!-- Customize your light theme here. -->
         <item name="colorPrimary">@color/green</item>
         <item name="colorPrimaryContainer">@color/green</item>
         <item name="colorOnSecondary">#FFedf5f3</item>
         <item name="colorSecondary">@color/orange</item>
         <item name="colorTertiaryContainer">#cde5ff</item>
         <item name="colorOnTertiaryContainer">#001d32</item>



        <item name="android:statusBarColor">?attr/colorSurface</item>
        <item name="android:windowLightStatusBar">true</item>
    </style>

    <style name="RoundedConrner">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">16dp</item>
    </style>

    <style name="ToolBar">
        <item name="android:fontFamily">@font/segeo_bold</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">16sp</item>

    </style>

    <style name="Theme.Proprioception" parent="Base.Theme.Proprioception" />
</resources>